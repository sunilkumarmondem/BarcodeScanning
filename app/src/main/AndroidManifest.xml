<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cognex.com.cmbcamerademo">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:iconifiedByDefault="false"
        android:theme="@style/AppTheme">
        <activity android:name=".ScannerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="MX_MOBILE_LICENSE"
            android:value="m/ymCWuskmAH4743nXzEPDNS95mW9S2hxZb/RM5GcPU=" />

        <activity android:name=".update_warehouse_activity" />
        <activity android:name=".add_product_activity" />
        <activity android:name=".allproducts_activity" />
        <activity android:name=".updateProduct_activity"></activity>
    </application>

</manifest>